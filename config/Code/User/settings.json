{
    "accessibility.dimUnfocused.enabled": true,
    "editor.formatOnPaste": true,
    "editor.formatOnSaveMode": "modificationsIfAvailable",
    "editor.formatOnSave": true,
    "editor.lineHeight": 1.5,
    "editor.minimap.enabled": false,
    "editor.unfoldOnClickAfterEndOfLine": true,
    "editor.wordWrapColumn": 100,
    "evenBetterToml.formatter.allowedBlankLines": 1,
    "evenBetterToml.formatter.arrayTrailingComma": true,
    "evenBetterToml.formatter.columnWidth": 100,
    "explorer.excludeGitIgnore": true,
    "explorer.fileNesting.enabled": true,
    "files.insertFinalNewline": true,
    "git.allowForcePush": true,
    "git.allowNoVerifyCommit": true,
    "git.alwaysShowStagedChangesResourceGroup": true,
    "git.branchPrefix": "benferse/",
    "git.branchProtection": [
        "develop"
    ],
    "git.branchRandomName.enable": true,
    "git.fetchOnPull": true,
    "git.followTagsWhenSync": true,
    "git.pruneOnFetch": true,
    "git.terminalGitEditor": true,
    "http.experimental.systemCertificatesV2": true,
    "problems.showCurrentInStatus": true,
    "rust-analyzer.check.command": "clippy",
    "scm.alwaysShowRepositories": true,
    "scm.experimental.showSyncView": true,
    "search.smartCase": true,
    "terminal.integrated.allowMnemonics": true,
    "terminal.integrated.lineHeight": 1.2,
    "vim.camelCaseMotion.enable": true,
    "vim.cursorStylePerMode.insert": "line",
    "vim.cursorStylePerMode.normal": "block",
    "vim.cursorStylePerMode.replace": "underline",
    "vim.cursorStylePerMode.visualblock": "block-outline",
    "vim.cursorStylePerMode.visual": "block-outline",
    "vim.cursorStylePerMode.visualline": "block-outline",
    "vim.easymotion": true,
    "vim.foldfix": true,
    "vim.hlsearch": true,
    "vim.leader": " ",
    "vim.replaceWithRegister": true,
    "vim.showMarksInGutter": true,
    "vim.smartRelativeLine": true,
    "vim.sneakReplacesF": true,
    "vim.sneak": true,
    "vim.sneakUseIgnorecaseAndSmartcase": true,
    "vim.targets.enable": true,
    "vim.textwidth": 100,
    "vim.useSystemClipboard": true,
    "window.dialogStyle": "custom",
    "window.titleBarStyle": "custom",
    "window.zoomLevel": 1,
    "workbench.activityBar.iconClickBehavior": "focus",
    "workbench.colorTheme": "Nord",
    "workbench.editor.autoLockGroups": {
        "mainThreadWebview-markdown.preview": true
    },
    "workbench.editor.openPositioning": "first",
    "workbench.editor.showTabs": false,
    "workbench.settings.useSplitJSON": true,
    "workbench.tree.renderIndentGuides": "always",
    "workbench.view.alwaysShowHeaderActions": true,
    "rust-analyzer.rust.analyzerTargetDir": true,
}
